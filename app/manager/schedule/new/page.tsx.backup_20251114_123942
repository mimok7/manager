  route: string; // Eì—´: ê²½ë¡œ
  carCode: string;
  carType: string;
  date: string; // Hì—´: ì¼ì
  time: string;
  airportName: string;
  flightNumber: string;
  passengerCount: number;
  carrierCount: number;
  placeName: string;
  stopover: string;
  carCount: number;
  unitPrice: number;
  totalPrice: number;
  email: string;
}

// í˜¸í…” (SH_H)
